{
    "name": "eject-all-disks-streamdeck",
    "version": "0.1.0",
    "description": "Stream Deck plugin to eject all external disks on macOS",
    "scripts": {
        "lint:check": "prettier . --check",
        "build": "rollup -c",
        "build:swift": "bash scripts/build-swift.sh",
        "watch": "rollup -c -w --watch.onEnd=\"streamdeck restart org.deverman.ejectalldisks\"",
        "lint:fix": "prettier . --write",
        "version:bump": "bash scripts/bump-version.sh"
    },
    "type": "module",
    "devDependencies": {
        "@elgato/cli": "^1.2.0",
        "@elgato/prettier-config": "^0.2.6",
        "@rollup/plugin-commonjs": "^28.0.0",
        "@rollup/plugin-node-resolve": "^15.2.2",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.0",
        "@tsconfig/node20": "^20.1.2",
        "@types/node": "~20.15.0",
        "prettier": "^3.4.2",
        "rollup": "^4.0.2",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2"
    },
    "dependencies": {
        "@elgato/streamdeck": "^2.0.0-beta.2"
    }
}
